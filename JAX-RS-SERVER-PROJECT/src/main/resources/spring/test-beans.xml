<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="testEmployeeBean" class="com.rest.server.testData.TestEmployeeBean">
        <property name="entities">
            <list>
                <bean id="ivanov" class="com.rest.server.entity.employee.Employee">
                    <property name="id" value="1"/>
                    <property name="firstName" value="Иван"/>
                    <property name="secondName" value="Иванов"/>
                    <property name="middleName" value="Иванович"/>
                </bean>
                <bean id="petrov" class="com.rest.server.entity.employee.Employee">
                    <property name="id" value="1"/>
                    <property name="firstName" value="Петр"/>
                    <property name="secondName" value="Петров"/>
                    <property name="middleName" value="Петрович"/>
                </bean>
            </list>
        </property>
    </bean>

</beans>